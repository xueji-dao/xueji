{
  "name": "@xueji/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "prepare": "husky",
    "commitlint": "commitlint --edit",
    "check-types": "tsc --noEmit --pretty",
    "commit": "git-cz",
    "release": "semantic-release --dry-run",
    "analyze": "cross-env ANALYZE=true nx build webui"
  },
  "private": true,
  "dependencies": {
    "@next/env": "^15.1.8",
    "next": "~15.2.5",
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@commitlint/cz-commitlint": "^19.8.1",
    "@eslint/compat": "^1.2.9",
    "@eslint/eslintrc": "^2.1.4",
    "@eslint/js": "^9.27.0",
    "@hyoban/eslint-plugin-tailwindcss": "4.0.0-alpha.11",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@next/bundle-analyzer": "^15.3.2",
    "@next/eslint-plugin-next": "^15.3.2",
    "@nx/devkit": "21.1.0",
    "@nx/eslint": "21.1.0",
    "@nx/eslint-plugin": "21.1.0",
    "@nx/jest": "21.1.0",
    "@nx/js": "21.1.0",
    "@nx/next": "21.1.0",
    "@nx/playwright": "21.1.0",
    "@nx/storybook": "21.1.0",
    "@nx/web": "21.1.0",
    "@nx/workspace": "21.1.0",
    "@playwright/test": "^1.52.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@swc-node/register": "~1.9.2",
    "@swc/cli": "~0.6.0",
    "@swc/core": "~1.5.29",
    "@swc/helpers": "~0.5.17",
    "@tailwindcss/postcss": "^4.1.7",
    "@testing-library/dom": "10.4.0",
    "@testing-library/react": "16.1.0",
    "@types/jest": "^29.5.14",
    "@types/node": "18.16.9",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "autoprefixer": "10.4.13",
    "babel-jest": "^29.7.0",
    "commitizen": "^4.3.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.27.0",
    "eslint-config-next": "^15.3.2",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-jsx-a11y": "6.10.1",
    "eslint-plugin-playwright": "^1.8.3",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "5.0.0",
    "eslint-plugin-testing-library": "^7.2.1",
    "husky": "^9.1.7",
    "inquirer": "9.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "nx": "21.1.0",
    "postcss": "8.4.38",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "semantic-release": "^24.2.4",
    "storybook": "^8.6.11",
    "tailwindcss": "^4.1.7",
    "ts-jest": "^29.3.4",
    "ts-node": "10.9.1",
    "tslib": "^2.8.1",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.32.1"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits"
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  },
  "packageManager": "pnpm@10.11.0"
}
